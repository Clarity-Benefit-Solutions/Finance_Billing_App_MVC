<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Finance Billing</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />

    <link href="~/css/DXR.css" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/a076d05399.js"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap-grid.css" />

    <link rel="stylesheet" href="~/css/vendor.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Site.css" />
    <link href="~/css/Bootstrap/CSS/bootstrap.min.css" rel="stylesheet" />

    <script src="~/js/Bootstrap/jquery.slim.min.js"></script>
    <script src="~/js/Bootstrap/popper.min.js"></script>

    <script src="~/lib/bootstrap/js/bootstrap.js" asp-append-version="true"></script>
    <script src="~/js/Bootstrap/Js/bootstrap.min.js"></script>

    <link href="~/css/Styles.css" rel="stylesheet" />
    <script src="~/js/vendor.js" asp-append-version="true"></script>

</head>

<body>
    <div class="border w-100 h-25">
        <img src="~/Clarity_Secondary.png.png" style="width: 200px; height: auto; ">
        <span style="font-weight: bolder; font-size: 28px; color: #2d5592; ">
            &nbsp;&nbsp;Finance Billing
        </span>
    </div>
    <div class="dxpnlControl_Moderno mainContentPane " id="MainPane">
        <div class="dxpnlControl_Moderno leftPane panel dxpnl-edge l" id="MainPane_Content_LeftPane">
            <div class="dxpnl-acc">
                <div class="leftPanel">
                    <ul class="dxnbLite_Moderno dxnbSys" id="MainPane_Content_LeftPane_ASPxNavBar1" style="border-width:0px;width:100%;padding-left:0px;padding-right:0px;padding-top:0px;padding-bottom:0px;">
                        <li class="dxnb-gr" style="margin-bottom:0px;">
                            <div class="dxnb-header">
                                <span class="fas fa-caret-right arrow" style="margin-right: 11px; "></span><span class="dxnb-ghtext dx-vam">DASHBOARD</span>
                            </div><ul class="dxnb-content dxnb-left">
                                <li class="dxnb-item dxnb-itemSelected dxnb-link dxnb-last"><span class="dx-vam"><a class="dxnb-link" href="http://BE014:301" target="_blank">Broker Application</a></span></li>
                            </ul>

                        </li>
                        <li class="dxnb-gr" style="margin-bottom:0px;">
                            <div class="dxnb-header">
                                <span class="fas fa-caret-right arrow" style="margin-right: 11px; "></span><span class="dxnb-ghtext dx-vam">NEW UPLOADS</span>
                            </div><ul class="dxnb-content dxnb-left">
                                <li class="dxnb-item dxnb-itemSelected dxnb-link dxnb-last"><a class="dxnb-link" asp-action="Quickbook" asp-controller="Quickbook"><span class="dx-vam">Quickbook Upload</span></a></li>
                                <li class="dxnb-item dxnb-itemSelected dxnb-link dxnb-last"><a class="dxnb-link" asp-action="UploadFile" asp-controller="UploadFile"><span class="dx-vam">Upload Files</span></a></li>

                            </ul>
                        </li>

                        <li class="dxnb-gr">
                            <div class="dxnb-header">
                                <span class="fas fa-caret-right arrow" style="margin-right: 11px; "></span><span class="dxnb-ghtext dx-vam">CLIENT DATA</span>
                            </div><ul class="dxnb-content dxnb-left">
                                <li class="dxnb-item dxnb-itemSelected dxnb-link dxnb-last"><a class="dxnb-link" asp-action="ExcludedClient" asp-controller="Client"><span class="dx-vam">Excluded Client</span></a></li>
                                <li class="dxnb-item dxnb-itemSelected dxnb-link dxnb-last"><a class="dxnb-link" asp-action="QuickbookClients" asp-controller="Quickbook"><span class="dx-vam">Quickbook Client</span></a></li>

                            </ul>
                        </li>

                        <li class="dxnb-gr">
                            <div class="dxnb-header">
                                <span class="fas fa-caret-right arrow" style="margin-right: 11px; "></span><span class="dxnb-ghtext dx-vam">OPEN BALANCE STATUS</span>
                            </div><ul class="dxnb-content dxnb-left">
                                <li class="dxnb-item dxnb-itemSelected dxnb-link dxnb-last"><a class="dxnb-link" asp-action="OpenBalStatus" asp-controller="OpenBalance"><span class="dx-vam">Open Balance Status</span></a></li>
                            
                            </ul>
                        </li>

                    </ul><b class="dx-clear"></b>

                </div>



            </div>
        </div>

        <div class="contentPane col-md-12" id="main">
            <h6 class="page-header-title text-sm-center">
                <span style="font-weight: bolder; font-size: 21px; color: #2d5592; ">
                    @ViewBag.Title
                </span>
            </h6>
            <main role="main" class="container">
                @RenderBody()
            </main>
            
        </div>
    </div>
      
</body>

</html>
